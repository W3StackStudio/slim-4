{
    "require": {
        "cocur/slugify": "^4.0",
        "intervention/image": "^2.5",
        "matomo/device-detector": "^4.0",
        "nesbot/carbon": "^2.32.0",
        "php-di/php-di": "^6.0",
        "razorpay/razorpay": "2.*",
        "selective/basepath": "^1.0",
        "selective/config": "^0.2.0",
        "slim/psr7": "^1.0",
        "slim/slim": "4.*",
        "tuupola/slim-jwt-auth": "^3.4",
        "vimeo/vimeo-api": "^3.0"
    },
    "require-dev": {
